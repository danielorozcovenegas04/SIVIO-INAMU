            <div id="home" class="tab-pane fade in active">
                <div class="row comunForm">
                    <div class="row">
                        <div class="col-xs-4 form-group">
                            <label>Nombre</label>
                            <input class="form-control" placeholder="Nombre" id="txtNombre" type="text" readonly>
                        </div>

                        <div class="col-xs-4 form-group">
                            <label>Primer Apellido</label>
                            <input class="form-control" placeholder="Primer Apellido" id="txtApellido1" type="text" readonly>
                        </div>

                        <div class="col-xs-4 form-group">
                            <label>Segundo Apellido</label>
                            <input class="form-control" placeholder="Segundo Apellido" id="txtApellido2" type="text" readonly>
                        </div>
                    </div>
                    <div class="row">
                        
                        <div class="col-xs-4 form-group">
                            <label>Cedula</label>
                            <input class="form-control" placeholder="Nombre" id="txtNombre" type="text" readonly>
                        </div>

                        <div class="col-xs-4 form-group">
                            <label>Telefono</label>
                            <input class="form-control" placeholder="Primer Apellido" id="txtApellido1" type="text" readonly>
                        </div>

                        <div class="col-xs-4 form-group">
                            <label>Fecha ultimo egreso</label>
                            <input class="form-control" placeholder="Segundo Apellido" id="txtApellido2" type="text" readonly>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-4 form-group">
                            <label>Provincia</label>
                            <input class="form-control" placeholder="Nombre" id="txtNombre" type="text" readonly>
                        </div>

                        <div class="col-xs-4 form-group">
                            <label>Canton</label>
                            <input class="form-control" placeholder="Primer Apellido" id="txtApellido1" type="text" readonly>
                        </div>

                        <div class="col-xs-4 form-group">
                            <label>Entrega del kit de emercencia</label>
                            <input class="form-control" placeholder="Segundo Apellido" id="txtApellido2" type="text" readonly>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12 form-group">
                            <label>Justificacion</label>
                            <input class="form-control" placeholder="Segundo Apellido" id="txtApellido2" type="text" required="">
                         </div>

                    </div>


                    <div class="row" id="contenedorBotones">
                        <div class="col-md-12" style="text-align: center; margin-bottom: 5px">
                            <button type="button" id="btnEnviarFormulario" class="btn btn-default btn-responsive" style="color: white; background-color: #368DE9; border-color: #368DE9; margin-bottom: 1px; ">Aceptar</button>
                    </div>


                    </div>
                </div>
            </div>
        </div>









    </form>

</div>

